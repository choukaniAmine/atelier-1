{% extends "base.html.twig" %}
{% block body %}
	<h1>Listes des articles</h1>
	<table class="table">
		<tr>
			<th>id</th>
			<th>libelle</th>
			<th>quantite</th>
			<th>gamme</th>
		</tr>
		{% for tab in articles %}
			<tr>
				{% for value in tab %}
					<td>{{value}}</td>
				{% endfor %}
			</tr>
		{% endfor %}
	</table>
{% endblock %}
